            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">import org.apache.jmeter.services.FileServer;
vars.put(&quot;toDiskCurflag&quot; , &quot;true&quot;);
vars.put(&quot;testPlanFileDir&quot;, FileServer.getFileServer().getBaseDir().replaceAll(&quot;\\\\&quot;, &quot;/&quot;));
boolean fileSuccessfullyDeleted = new File(vars.get(&quot;testPlanFileDir&quot;)+&quot;/&quot;+vars.get(&quot;packagePath&quot;)).delete()
</stringProp>
